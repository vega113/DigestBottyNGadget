<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:com.aggfi.digest.client.ui">
	<ui:with field='res'
		type='com.aggfi.digest.client.resources.GlobalResources' />
	<g:HTMLPanel>
		<g:VerticalPanel width="400px">
			<g:VerticalPanel>
				<g:SimplePanel ui:field="prjListContainer"></g:SimplePanel>
				<g:HorizontalPanel>
					<g:CaptionPanel captionText='Select Report type'>
						<g:HorizontalPanel>
							<g:HTML width="10px"></g:HTML>
							<g:ListBox ui:field="reportTypesList"></g:ListBox>
						</g:HorizontalPanel>
					</g:CaptionPanel>
				</g:HorizontalPanel>
				<g:VerticalPanel>
					<g:DecoratorPanel>
						<g:VerticalPanel ui:field="reportPanel"></g:VerticalPanel>
					</g:DecoratorPanel>
				</g:VerticalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
