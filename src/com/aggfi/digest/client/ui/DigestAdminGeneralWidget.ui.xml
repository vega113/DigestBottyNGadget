<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res'
		type='com.aggfi.digest.client.resources.GlobalResources' />
	<ui:with field='constants'
		type='com.aggfi.digest.client.constants.DigestConstants' />
	<g:HTMLPanel>
		<g:VerticalPanel width='{constants.smallerWidthStr}'
			stylePrimaryName='{res.globalCSS.highlightRow}'>
			<g:SimplePanel ui:field="prjListContainer"></g:SimplePanel>

				<g:VerticalPanel>
					<g:CaptionPanel width='{constants.smallerWidthStr}'>
						<g:HorizontalPanel>
							<g:Label text='{constants.makeTheFeedPublicQuestionStr}'></g:Label>
							<g:CheckBox ui:field="isAtomFeedPublicCheckBox"></g:CheckBox>
							<g:Image resource='{res.tooltip}' title='{constants.isAtomFeedPublicTagExpl}'
								ui:field="img9" />
							<g:HorizontalPanel>
								<g:HTML width="188px"></g:HTML>
								<g:Button text='{constants.updateStr}' ui:field="isAtomFeedPublicUpdateBtn"></g:Button>
							</g:HorizontalPanel>
						</g:HorizontalPanel>
					</g:CaptionPanel>
					<g:VerticalPanel>
						<g:HTML height="10px"></g:HTML>
					</g:VerticalPanel>
					<g:CaptionPanel width='{constants.smallerWidthStr}'>
						<g:HorizontalPanel>
							<g:Label text='{constants.addSecureGadgetQuestionStr}'></g:Label>
							<g:HTML width='6px'></g:HTML>
							<g:Image resource='{res.tooltip}' title='{constants.addSecureGadgetExpl}'
								ui:field="img10" />
							<g:HorizontalPanel>
								<g:HTML width='214px'></g:HTML>
								<g:Button text='{constants.addBtnName}' ui:field="addSecurePostGadgetBtn"></g:Button>
							</g:HorizontalPanel>
						</g:HorizontalPanel>
					</g:CaptionPanel>
					<g:VerticalPanel>
						<g:HTML height="10px"></g:HTML>
					</g:VerticalPanel>
					<g:CaptionPanel width='{constants.smallerWidthStr}'>
						<g:VerticalPanel>
							<g:HorizontalPanel>
							<g:Label text='{constants.addSocialBtnsQuestion}'></g:Label>
							<g:HTML width='6px'></g:HTML>
							<g:Image resource='{res.tooltip}' title='{constants.addSocialBtnstExpl}'
								ui:field="img11" />
							<g:HorizontalPanel>
								<g:HTML width='105px'></g:HTML>
								<g:Button text='{constants.updateStr}' ui:field="isSocialBtnsUpdateBtn"></g:Button>
							</g:HorizontalPanel>
						</g:HorizontalPanel>
						<g:HorizontalPanel>
							<g:HorizontalPanel>
								<g:Label text='{constants.digg}'></g:Label>
								<g:CheckBox checked='true' ui:field="isDiggEnabledCheckBox"></g:CheckBox>
							</g:HorizontalPanel>
							<g:HTML width='6px'></g:HTML>
							<g:HorizontalPanel>
								<g:Label text='{constants.buzz}'></g:Label>
								<g:CheckBox checked='true' ui:field="isBuzzEnabledCheckBox"></g:CheckBox>
							</g:HorizontalPanel>
							<g:HTML width='6px'></g:HTML>
							<g:HorizontalPanel>
								<g:Label text='{constants.tweet}'></g:Label>
								<g:CheckBox checked='true' ui:field="isTweetEnabledCheckBox"></g:CheckBox>
							</g:HorizontalPanel>
							<g:HTML width='6px'></g:HTML>
							<g:HorizontalPanel>
								<g:Label text='{constants.face}'></g:Label>
								<g:CheckBox checked='true' ui:field="isFaceEnabledCheckBox"></g:CheckBox>
							</g:HorizontalPanel>
						</g:HorizontalPanel>
						</g:VerticalPanel>
					</g:CaptionPanel>
				</g:VerticalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 