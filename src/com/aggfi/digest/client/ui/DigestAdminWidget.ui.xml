<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:digest="urn:import:com.aggfi.digest.client.ui"
	>
	<ui:with field='res'
		type='com.aggfi.digest.client.resources.GlobalResources' />
	<ui:with field='constants'
		type='com.aggfi.digest.client.constants.DigestConstants' />
	<g:HTMLPanel>
		<g:VerticalPanel width='{constants.smallerWidthStr}' stylePrimaryName='{res.globalCSS.highlightRow}'>
			<digest:DigestAdminSimplePanel ui:field="prjListContainer"></digest:DigestAdminSimplePanel> 
			<g:TabPanel ui:field="adminSettingsTabPnl" width='{constants.smallerWidthStr}'>
			</g:TabPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 