<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res'
		type='com.aggfi.digest.client.resources.GlobalResources' />
	<ui:with field='constants'
		type='com.aggfi.digest.client.constants.DigestConstants' />
	<g:HTMLPanel>
		<g:VerticalPanel width='{constants.smallerBiggerWidthStr}'
			stylePrimaryName='{res.globalCSS.highlightRow}'>
			<g:SimplePanel ui:field="prjListContainer"></g:SimplePanel>
			<g:CaptionPanel captionText='{constants.setUpDefaultParticipantsStr}'>
				<g:VerticalPanel>
					<g:HorizontalPanel>
						<g:TextBox ui:field="addDefaultParticipantBox" title='{constants.defaultParticipantExpl}'
							width='{constants.basicTextBoxWidthStr}'></g:TextBox>
						<g:Image resource='{res.tooltip}' title='{constants.defaultParticipantExpl}'
							ui:field="img1" />
						<g:Button ui:field="addDefaultParticipantBtn" text='{constants.addBtnName}'></g:Button>
					</g:HorizontalPanel>
					<g:cell height="10px">
						<g:HTML></g:HTML>
					</g:cell>
					<g:ScrollPanel width='{constants.smallerWidthStr}'
						height='{constants.basicItemScrollHeightStr}'>
						<g:VerticalPanel ui:field="defaultParticipantsPanel">
						</g:VerticalPanel>
					</g:ScrollPanel>
				</g:VerticalPanel>
			</g:CaptionPanel>

			<g:CaptionPanel captionText='{constants.setUpDefaultTagsStr}'
				ui:field="defaultTagsCaptPnl">
				<g:VerticalPanel>
					<g:HorizontalPanel>
						<g:TextBox ui:field="addDefaultTagBox" title='{constants.defaultTagExpl}'
							width='{constants.basicTextBoxWidthStr}'></g:TextBox>
						<g:Image resource='{res.tooltip}' title='{constants.defaultTagExpl}'
							ui:field="img2" />
						<g:Button ui:field="addDefaultTagBtn" text='{constants.addBtnName}'></g:Button>
					</g:HorizontalPanel>
					<g:cell height="10px">
						<g:HTML></g:HTML>
					</g:cell>
					<g:ScrollPanel width='{constants.smallerWidthStr}'
						height='{constants.basicItemScrollHeightStr}'>
						<g:VerticalPanel ui:field="defaultTagsPanel">
						</g:VerticalPanel>
					</g:ScrollPanel>
				</g:VerticalPanel>
			</g:CaptionPanel>
			<g:CaptionPanel captionText='{constants.setUpDigestManagers}'>
				<g:VerticalPanel>
					<g:HorizontalPanel>
						<g:TextBox ui:field="addManagerBox" title='{constants.managersExpl}'
							width='{constants.basicTextBoxWidthStr}'></g:TextBox>
						<g:Image resource='{res.tooltip}' title='{constants.managersExpl}'
							ui:field="img3" />
						<g:Button ui:field="addManagergBtn" text='{constants.addBtnName}'></g:Button>
					</g:HorizontalPanel>
					<g:cell height="10px">
						<g:HTML></g:HTML>
					</g:cell>
					<g:ScrollPanel width='{constants.smallerWidthStr}'
						height='{constants.basicItemScrollHeightStr}'>
						<g:VerticalPanel ui:field="managersPanel">
						</g:VerticalPanel>
					</g:ScrollPanel>
				</g:VerticalPanel>
			</g:CaptionPanel>
			<g:CaptionPanel captionText='{constants.setUpAutoTagging}'>
				<g:VerticalPanel>
					<g:HorizontalPanel>
						<g:VerticalPanel>
							<g:HorizontalPanel>
								<g:Label text='{constants.tagStr}'></g:Label>
								<g:Label>:</g:Label>
							</g:HorizontalPanel>
							<g:HorizontalPanel>
								<g:TextBox ui:field="addAutoTagNameBox" title='{constants.autoTagExpl}'
									width='{constants.basicTextBoxWidthStr}'></g:TextBox>
								<g:Image resource='{res.tooltip}' title='{constants.autoTagNameExpl}'
									ui:field="img4" />
							</g:HorizontalPanel>
						</g:VerticalPanel>
						<g:HTML width="10px"></g:HTML>
						<g:VerticalPanel>
							<g:HorizontalPanel>
								<g:Label text='{constants.regexStr}'></g:Label>
								<g:Label>:</g:Label>
							</g:HorizontalPanel>
							<g:HorizontalPanel>
								<g:TextBox ui:field="addAutoTagValBox" title='{constants.autoTagExpl}'
									width='{constants.basicTextBoxWidthStr}'></g:TextBox>
								<g:Image resource='{res.tooltip}' title='{constants.autoTagExpl}'
									ui:field="img5" />
								<g:Button ui:field="addAutoTagBtn" text='{constants.addBtnName}'></g:Button>
							</g:HorizontalPanel>
						</g:VerticalPanel>
						<g:HTML width="1px"></g:HTML>
						<g:CaptionPanel>
							<g:VerticalPanel>
								<g:HorizontalPanel>
									<g:Label text='{constants.syncStr}'></g:Label>
								</g:HorizontalPanel>
								<g:HorizontalPanel>
									<g:CheckBox ui:field="syncAutoTagCheckBox"></g:CheckBox>
									<g:Image resource='{res.tooltip}' title='{constants.autoTagSyncExpl}'
										ui:field="img6" />
								</g:HorizontalPanel>
							</g:VerticalPanel>
						</g:CaptionPanel>
					</g:HorizontalPanel>
					<g:cell height="10px">
						<g:HTML></g:HTML>
					</g:cell>
					<g:ScrollPanel width='{constants.smallerWidthStr}'
						height='{constants.basicItemScrollHeightStr}'>
						<g:VerticalPanel ui:field="autoTagsPanel">
						</g:VerticalPanel>
					</g:ScrollPanel>
				</g:VerticalPanel>
			</g:CaptionPanel>

			<g:CaptionPanel captionText='{constants.setUpParticipantWaves}'
				ui:field="addParticipantWaveCaption">
				<g:VerticalPanel>
					<g:HorizontalPanel>
						<g:VerticalPanel>
							<g:HorizontalPanel>
								<g:Label text='{constants.participantsStr}'></g:Label>
								<g:Label>:</g:Label>
							</g:HorizontalPanel>
							<g:HorizontalPanel>
								<g:TextBox ui:field="addParticipantWavesBox" title='{constants.participantWavesExpl}'
									width='{constants.basicTextBoxWidthStr}'></g:TextBox>
								<g:Image resource='{res.tooltip}' title='{constants.participantWavesExpl}'
									ui:field="img7" />
							</g:HorizontalPanel>
						</g:VerticalPanel>
						<g:HTML width="10px"></g:HTML>
						<g:VerticalPanel>
							<g:HorizontalPanel>
								<g:Label text='{constants.tagStr}'></g:Label>
								<g:Label>:</g:Label>
							</g:HorizontalPanel>
							<g:HorizontalPanel>
								<g:TextBox ui:field="addParticipanTagtWavesBox" title='{constants.participantWavesExpl}'
									width='{constants.basicTextBoxWidthStr}'></g:TextBox>
								<g:Image resource='{res.tooltip}' title='{constants.participantWavesTagExpl}'
									ui:field="img8" />
								<g:Button ui:field="addParticipantWavesBtn" text='{constants.addBtnName}'></g:Button>
							</g:HorizontalPanel>
						</g:VerticalPanel>
					</g:HorizontalPanel>
					<g:cell height="10px">
						<g:HTML></g:HTML>
					</g:cell>
					<g:ScrollPanel width='{constants.smallerWidthStr}'
						height='{constants.basicItemScrollHeightStr}'>
						<g:VerticalPanel ui:field="participantWavesPanel">
						</g:VerticalPanel>
					</g:ScrollPanel>
				</g:VerticalPanel>
			</g:CaptionPanel>
			<g:CaptionPanel captionText='{constants.setUpAtomFeedStr}'>
				<g:VerticalPanel>
					<g:CaptionPanel width='{constants.smallerWidthStr}'>
						<g:HorizontalPanel>
							<g:Label text='{constants.makeTheFeedPublicQuestionStr}'></g:Label>
							<g:CheckBox ui:field="isAtomFeedPublicCheckBox"></g:CheckBox>
							<g:Image resource='{res.tooltip}' title='{constants.isAtomFeedPublicTagExpl}'
								ui:field="img9" />
							<g:HorizontalPanel>
								<g:HTML width="188px"></g:HTML>
								<g:Button text='{constants.updateStr}' ui:field="isAtomFeedPublicUpdateBtn"></g:Button>
							</g:HorizontalPanel>
						</g:HorizontalPanel>
					</g:CaptionPanel>
					<g:VerticalPanel>
						<g:HTML height="10px"></g:HTML>
					</g:VerticalPanel>
					<g:CaptionPanel width='{constants.smallerWidthStr}'>
						<g:HorizontalPanel>
							<g:Label text='{constants.addSecureGadgetQuestionStr}'></g:Label>
							<g:HTML width='6px'></g:HTML>
							<g:Image resource='{res.tooltip}' title='{constants.addSecureGadgetExpl}'
								ui:field="img10" />
							<g:HorizontalPanel>
								<g:HTML width='214px'></g:HTML>
								<g:Button text='{constants.addBtnName}' ui:field="addSecurePostGadgetBtn"></g:Button>
							</g:HorizontalPanel>
						</g:HorizontalPanel>
					</g:CaptionPanel>
					<g:VerticalPanel>
						<g:HTML height="10px"></g:HTML>
					</g:VerticalPanel>
					<g:CaptionPanel width='{constants.smallerWidthStr}'>
						<g:VerticalPanel>
							<g:HorizontalPanel>
							<g:Label text='{constants.addSocialBtnsQuestion}'></g:Label>
							<g:HTML width='6px'></g:HTML>
							<g:Image resource='{res.tooltip}' title='{constants.addSocialBtnstExpl}'
								ui:field="img11" />
							<g:HorizontalPanel>
								<g:HTML width='140px'></g:HTML>
								<g:Button text='{constants.updateStr}' ui:field="isSocialBtnsUpdateBtn"></g:Button>
							</g:HorizontalPanel>
						</g:HorizontalPanel>
						<g:HorizontalPanel>
							<g:HorizontalPanel>
								<g:Label text='{constants.digg}'></g:Label>
								<g:CheckBox checked='true' ui:field="isDiggEnabledCheckBox"></g:CheckBox>
							</g:HorizontalPanel>
							<g:HTML width='6px'></g:HTML>
							<g:HorizontalPanel>
								<g:Label text='{constants.buzz}'></g:Label>
								<g:CheckBox checked='true' ui:field="isBuzzEnabledCheckBox"></g:CheckBox>
							</g:HorizontalPanel>
							<g:HTML width='6px'></g:HTML>
							<g:HorizontalPanel>
								<g:Label text='{constants.tweet}'></g:Label>
								<g:CheckBox checked='true' ui:field="isTweetEnabledCheckBox"></g:CheckBox>
							</g:HorizontalPanel>
							<g:HTML width='6px'></g:HTML>
							<g:HorizontalPanel>
								<g:Label text='{constants.face}'></g:Label>
								<g:CheckBox checked='true' ui:field="isFaceEnabledCheckBox"></g:CheckBox>
							</g:HorizontalPanel>
						</g:HorizontalPanel>
						</g:VerticalPanel>
					</g:CaptionPanel>
				</g:VerticalPanel>
			</g:CaptionPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 