<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res'
		type='com.aggfi.digest.client.resources.GlobalResources' />
	<ui:with field='constants'
		type='com.aggfi.digest.client.constants.SimpleConstants' />
	<g:HTMLPanel>
		<g:VerticalPanel width='{constants.basicWidthStr}' stylePrimaryName='{res.globalCSS.highlightRow}'>
			<g:SimplePanel ui:field="prjListContainer"></g:SimplePanel>
			<g:CaptionPanel captionText='{constants.setUpDefaultParticipantsStr}'>
				<g:VerticalPanel>
					<g:HorizontalPanel>
						<g:TextBox ui:field="addDefaultParticipantBox" title='{constants.defaultParticipantExpl}'></g:TextBox>
						<g:Button ui:field="addDefaultParticipantBtn" text='{constants.addBtnName}'></g:Button>
					</g:HorizontalPanel>
					<g:cell height="10px">
						<g:HTML></g:HTML>
					</g:cell>
					<g:VerticalPanel ui:field="defaultParticipantsPanel">
					</g:VerticalPanel>
				</g:VerticalPanel>
			</g:CaptionPanel>

			<g:CaptionPanel captionText='{constants.setUpDefaultTagsStr}'>
				<g:VerticalPanel>
					<g:HorizontalPanel>
						<g:TextBox ui:field="addDefaultTagBox" title='{constants.defaultTagExpl}'></g:TextBox>
						<g:Button ui:field="addDefaultTagBtn" text='{constants.addBtnName}'></g:Button>
					</g:HorizontalPanel>
					<g:cell height="10px">
						<g:HTML></g:HTML>
					</g:cell>
					<g:VerticalPanel ui:field="defaultTagsPanel">
					</g:VerticalPanel>
				</g:VerticalPanel>
			</g:CaptionPanel>

			<g:CaptionPanel captionText='{constants.setUpAutoTagging}'>
				<g:VerticalPanel>
					<g:HorizontalPanel>
						<g:Label text='{constants.tagStr}'></g:Label>
						<g:Label>:</g:Label>
						<g:HTML width="4px"></g:HTML>
						<g:TextBox ui:field="addAutoTagNameBox" title='{constants.autoTagExpl}'></g:TextBox>
						<g:HTML width="10px"></g:HTML>
						<g:Label text='{constants.regexStr}'></g:Label>
						<g:Label>:</g:Label>
						<g:HTML width="4px"></g:HTML>
						<g:TextBox ui:field="addAutoTagValBox" title='{constants.autoTagExpl}'></g:TextBox>
						<g:Button ui:field="addAutoTagBtn" text='{constants.addBtnName}'></g:Button>
					</g:HorizontalPanel>
					<g:cell height="10px">
						<g:HTML></g:HTML>
					</g:cell>
					<g:VerticalPanel ui:field="autoTagsPanel">
					</g:VerticalPanel>
				</g:VerticalPanel>
			</g:CaptionPanel>

			<g:CaptionPanel captionText='{constants.setUpDigestManagers}'>
				<g:VerticalPanel>
					<g:HorizontalPanel>
						<g:TextBox ui:field="addManagerBox" title='{constants.managersExpl}'></g:TextBox>
						<g:Button ui:field="addManagergBtn" text='{constants.addBtnName}'></g:Button>
					</g:HorizontalPanel>
					<g:cell height="10px">
						<g:HTML></g:HTML>
					</g:cell>
					<g:VerticalPanel ui:field="managersPanel">
					</g:VerticalPanel>
				</g:VerticalPanel>
			</g:CaptionPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 