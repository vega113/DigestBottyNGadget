<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res'
		type='com.aggfi.digest.client.resources.GlobalResources' />
	<ui:with field='constants'
		type='com.aggfi.digest.client.constants.DigestConstants' />
	<g:HTMLPanel>
		<g:HorizontalPanel stylePrimaryName='{res.globalCSS.highlightRow}' width='{constants.basicWidthStr}'>
			<g:CaptionPanel captionText='{constants.selectDigestStr}' ui:field="outerCptnPnl" width='{constants.smallerBiggerWidthStr}'>
					<g:HorizontalPanel>
						<g:HTML width="10px"></g:HTML>
						<g:ListBox ui:field="prjList" width='{constants.reportListWidthStr}'></g:ListBox>
						<g:HorizontalPanel ui:field="refresBtnhPnl">
								<g:HTML width='15px'></g:HTML>
						</g:HorizontalPanel>
						<g:HorizontalPanel ui:field="goBtnhPnl">
								<g:HTML width='5px'></g:HTML>
						</g:HorizontalPanel>
					</g:HorizontalPanel>
		</g:CaptionPanel>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 